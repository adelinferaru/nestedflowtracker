{
    "name": "adelinferaru/nestedflowtracker",
    "description": "Laravel package that helps tracking nested execution flows.",
    "license": "MIT",
    "authors": [
        {
            "name": "Feraru Ioan Adelin",
            "email": "alu@feenavigator.com",
            "homepage": "https://www.feenavigator.com"
        }
    ],
    "homepage": "https://github.com/adelinferaru/nestedflowtracker",
    "keywords": ["Laravel", "NestedFlowTracker"],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0",
        "kalnoy/nestedset": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "AdelinFeraru\\NestedFlowTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdelinFeraru\\NestedFlowTracker\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdelinFeraru\\NestedFlowTracker\\NestedFlowTrackerServiceProvider"
            ],
            "aliases": {
                "NestedFlowTracker": "AdelinFeraru\\NestedFlowTracker\\Facades\\NestedFlowTracker"
            }
        }
    }
}
